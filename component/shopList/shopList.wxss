
.shopListContent {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.shopListImg image {
  width: 100rpx;
  height: 100rpx;
}
.shopListImg {
  display: flex;
  justify-content: center;
  align-items: center;
}
.shopListTitle {
  font-size: 30rpx;
  color: #333333;
  font-weight: bold;
}
.shopListText,.shopListm{
  color: #999999;
  font-size: 24rpx;
}
.shopListText {
  margin-top: 10rpx;
}
.shopList {
  display: flex;
  align-items: center;
  background: #ffffff;
  width: 710rpx;
  justify-content: space-between;
  height: 140rpx;
  margin-top: 20rpx;
}
.shopListOne {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 80rpx;
}
.shopTree {
  margin-right: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}
.shopListTwo {
  display: flex;
  align-items: center;
  margin-left: 20rpx;
}
.shopListJt image {
  width: 20rpx;
  height: 20rpx;
}
.shopListOpen {
  display: flex;
  align-items: center;
  color: #de443a;
  border: 2rpx #de443a solid;
  padding-left: 28rpx;
  padding-right: 28rpx;
  padding-bottom: 10rpx;
  border-radius: 50rpx;
  font-size: 26rpx;
  padding-top: 8rpx;
  margin-top: 20rpx;
}